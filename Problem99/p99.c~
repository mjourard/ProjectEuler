#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <math.h>


int main(void) {
  FILE * numFile;
  char buffer[50];
  double biggestNumber, curNum, curExpo;

  numFile = fopen("base_exp.txt", "r");
  if (numFile == NULL) {
    printf("Error opening file.\n");
    exit(1);
  }
  
  while(fgets(buffer, 50, numFile) != NULL) {
    printf("%s\n", buffer);
  }



  return 0;
}

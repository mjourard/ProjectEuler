#include <stdio.h>
#include <stdlib.h>

char sameDigitCheck(int[] n1, int[] n2);
/* returns a 1 if the digits are the same, 0 if not */

int main(void) {
  
  return 0;
} 

char sameDigitCheck(int[] n1, int[] n2, int size) {
  int i, j, check;
  for (i = 0; i < size; i++) {
    check = 0;
    for (j = 0; j < size; j++) {
      if (n1[i] == n2[j]) {
        check = 1;
        break;
      }
    }
    if (check == 0) {
      return 0;
    }
  }
  return 1;
  
}
